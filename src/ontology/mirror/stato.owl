    <!-- http://purl.obolibrary.org/obo/OBI_0000066 -->

    <owl:Class rdf:about="http://purl.obolibrary.org/obo/OBI_0000066">
        <rdfs:label xml:lang="en">investigation</rdfs:label>
        <rdfs:subClassOf rdf:resource="http://purl.obolibrary.org/obo/OBI_0000011"/>
        <obo:IAO_0000118 rdf:datatype="http://www.w3.org/2001/XMLSchema#string">study</obo:IAO_0000118>
        <obo:IAO_0000115 xml:lang="en">a planned process that consists of parts: planning, study design execution, documentation and which produce conclusion(s).</obo:IAO_0000115>
